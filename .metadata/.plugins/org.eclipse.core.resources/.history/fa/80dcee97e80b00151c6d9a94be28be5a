package automationMobile;

public class compareList {
	
	public interface Comparable<T> {
	    int compareTo(T outro);
	}
	
	public Conta implements Comparable<Conta> {
        
	    private string numero;
	    private String titular;
	    // outros metodos e atributos
	 
	    public int compareTo(sortByName outraConta) {
	        if (this.numero.equals(sortByName.allItems)) {
	            return -1;
	        }
	        if (this.numero > sortByName.allItems) {
	            return 1;
	        }
	        return 0;
	    }
	}

}
